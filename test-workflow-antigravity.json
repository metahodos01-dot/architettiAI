{
    "name": "ðŸ§ª Test Workflow - Antigravity Integration",
    "nodes": [
        {
            "id": "webhook-1",
            "name": "Webhook Trigger",
            "type": "n8n-nodes-base.webhook",
            "typeVersion": 2,
            "position": [
                250,
                300
            ],
            "parameters": {
                "httpMethod": "POST",
                "path": "test-antigravity",
                "responseMode": "responseNode"
            }
        },
        {
            "id": "code-1",
            "name": "Process Data",
            "type": "n8n-nodes-base.code",
            "typeVersion": 2,
            "position": [
                450,
                300
            ],
            "parameters": {
                "jsCode": "const input = $input.first().json;\nconst timestamp = new Date().toISOString();\n\nreturn [{\n  json: {\n    message: \"Processed by Antigravity Integration\",\n    originalData: input,\n    processedAt: timestamp,\n    status: input.value > 50 ? \"high\" : \"low\"\n  }\n}];"
            }
        },
        {
            "id": "if-1",
            "name": "Check Status",
            "type": "n8n-nodes-base.if",
            "typeVersion": 2,
            "position": [
                650,
                300
            ],
            "parameters": {
                "conditions": {
                    "options": {
                        "caseSensitive": true,
                        "leftValue": "",
                        "typeValidation": "strict"
                    },
                    "conditions": [
                        {
                            "id": "condition-1",
                            "leftValue": "={{ $json.status }}",
                            "rightValue": "high",
                            "operator": {
                                "type": "string",
                                "operation": "equals"
                            }
                        }
                    ],
                    "combinator": "and"
                }
            }
        },
        {
            "id": "set-high",
            "name": "High Priority Response",
            "type": "n8n-nodes-base.set",
            "typeVersion": 3.4,
            "position": [
                850,
